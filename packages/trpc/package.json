{
  "name": "@demokit-ai/trpc",
  "version": "0.3.0",
  "description": "tRPC integration for DemoKit",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    },
    "./react": {
      "import": { "types": "./dist/react.d.ts", "default": "./dist/react.js" },
      "require": { "types": "./dist/react.d.cts", "default": "./dist/react.cjs" }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --no-dts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@demokit-ai/core": "workspace:*",
    "@demokit-ai/react": "workspace:*"
  },
  "devDependencies": {
    "@trpc/client": "^11.0.0-rc.532",
    "@trpc/react-query": "^11.0.0-rc.532",
    "@trpc/server": "^11.0.0-rc.532",
    "@types/react": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@trpc/client": ">=11.0.0-rc.0",
    "@trpc/react-query": ">=11.0.0-rc.0",
    "@trpc/server": ">=11.0.0-rc.0",
    "react": ">=17.0.0"
  },
  "engines": { "node": ">=18" },
  "license": "Apache-2.0",
  "sideEffects": false
}
