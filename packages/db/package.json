{
  "name": "@demokit-ai/db",
  "version": "0.3.0",
  "description": "Database schema and utilities for DemoKit (PostgreSQL + pgvector with Drizzle ORM). Schema-only package - migrations handled by consuming apps.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    },
    "./schema": {
      "import": { "types": "./dist/schema/index.d.ts", "default": "./dist/schema/index.js" },
      "require": { "types": "./dist/schema/index.d.cts", "default": "./dist/schema/index.cjs" }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --no-dts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "drizzle-orm": "^0.33.0",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "drizzle-kit": "^0.24.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": { "node": ">=18" },
  "license": "Apache-2.0",
  "sideEffects": false
}
