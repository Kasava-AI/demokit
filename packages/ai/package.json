{
  "name": "@demokit-ai/ai",
  "version": "0.3.0",
  "description": "AI-powered narrative generation for DemoKit (L3 generation with BYOK support)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --no-dts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@demokit-ai/core": "workspace:*",
    "@ai-sdk/anthropic": "^1.0.0",
    "@mastra/core": "^1.0.0-beta.19",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@mastra/langfuse": ">=0.1.0",
    "langfuse": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@mastra/langfuse": {
      "optional": true
    },
    "langfuse": {
      "optional": true
    }
  },
  "engines": { "node": ">=18" },
  "license": "Apache-2.0",
  "sideEffects": false
}
